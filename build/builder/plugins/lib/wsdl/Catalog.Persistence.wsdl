      <part name="class_names" type="zeusns:StringArray" />
   </message>
   
   <message name="getUrlByLocationResponse">
      <part name="values" type="zeusns:StringArray" />
   </message>
   
   <message name="renamePersistenceRequest">
      <part name="class_names" type="zeusns:StringArray" />
      <part name="new_names" type="zeusns:StringArray" />
   </message>
   
   <message name="renamePersistenceResponse"></message>
   
   <message name="setCookieRequest">
      <part name="class_names" type="zeusns:StringArray" />
      <part name="values" type="zeusns:StringArray" />
   </message>
   
   <message name="setCookieResponse"></message>
   
