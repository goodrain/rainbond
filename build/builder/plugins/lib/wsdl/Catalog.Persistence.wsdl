   <message name="getFailureModeByLocationResponse">
      <part name="values" type="zeusns:Catalog.Persistence.FailureModeArray" />
   </message>
   
   <message name="getNoteRequest">
      <part name="class_names" type="zeusns:StringArray" />
   </message>
   
   <message name="getNoteResponse">
      <part name="values" type="zeusns:StringArray" />
   </message>
   
   <message name="getPersistenceNamesRequest"></message>
   
   <message name="getPersistenceNamesResponse">
      <part name="class_names" type="zeusns:StringArray" />
   </message>
   
   <message name="getTypeRequest">
      <part name="class_names" type="zeusns:StringArray" />
   </message>
   
   <message name="getTypeResponse">
