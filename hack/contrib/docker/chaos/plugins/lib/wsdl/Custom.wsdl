         <fault name="ObjectDoesNotExist">
            <soap:fault use="encoded" name="ObjectDoesNotExist" namespace="http://soap.zeus.com/zxtm/1.0/Custom/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
