            <soap:body use="encoded" namespace="http://soap.zeus.com/zxtm/1.0/Custom/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
         </input>
         <output>
            <soap:body use="encoded" namespace="http://soap.zeus.com/zxtm/1.0/Custom/" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
