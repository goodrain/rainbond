                        <xsd:documentation>Fault Tolerance - All monitored OSPF neighbors are peered</xsd:documentation>
                     </xsd:annotation>
                  </xsd:enumeration>
               
                  <xsd:enumeration value="faulttolerance_pingbackendfail">
                     <xsd:annotation>
                        <xsd:documentation>Fault Tolerance - Failed to ping back-end nodes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:enumeration>
               
