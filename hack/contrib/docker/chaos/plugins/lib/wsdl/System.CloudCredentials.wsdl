         </documentation>
         
         <input message="zeusns:getApiServerByLocationRequest"/>
         <output message="zeusns:getApiServerByLocationResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
      </operation>
      
