   <portType name="Catalog.SLMPort">
      <operation name="addSLM">
         <documentation>
            Add new SLM classes.
         </documentation>
         
         <input message="zeusns:addSLMRequest"/>
         <output message="zeusns:addSLMResponse"/>
         <fault name="InvalidObjectName" message="zeusns:InvalidObjectName"/>
         <fault name="ObjectAlreadyExists" message="zeusns:ObjectAlreadyExists"/>
         <fault name="DeploymentError" message="zeusns:DeploymentError"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="copySLM">
         <documentation>
            Copy the named SLM classes.
         </documentation>
         
         <input message="zeusns:copySLMRequest"/>
         <output message="zeusns:copySLMResponse"/>
         <fault name="ObjectAlreadyExists" message="zeusns:ObjectAlreadyExists"/>
         <fault name="InvalidObjectName" message="zeusns:InvalidObjectName"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="DeploymentError" message="zeusns:DeploymentError"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="deleteSLM">
         <documentation>
            Delete the named SLM classes.
         </documentation>
         
         <input message="zeusns:deleteSLMRequest"/>
         <output message="zeusns:deleteSLMResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="ObjectInUse" message="zeusns:ObjectInUse"/>
         <fault name="DeploymentError" message="zeusns:DeploymentError"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="getNote">
         <documentation>
            Get the note for each of the named SLM classes.
         </documentation>
         
         <input message="zeusns:getNoteRequest"/>
         <output message="zeusns:getNoteResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="getResponseTime">
         <documentation>
            Get the time limit for a response to conform (in milliseconds).
         </documentation>
         
         <input message="zeusns:getResponseTimeRequest"/>
         <output message="zeusns:getResponseTimeResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="getResponseTimeByLocation">
         <documentation>
            Get the time limit for a response to conform (in milliseconds). This is a location specific function, any action will operate on the specified location.
         </documentation>
         
         <input message="zeusns:getResponseTimeByLocationRequest"/>
         <output message="zeusns:getResponseTimeByLocationResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="getSLMNames">
         <documentation>
            Get the names of all the configured SLM classes.
         </documentation>
         
         <input message="zeusns:getSLMNamesRequest"/>
         <output message="zeusns:getSLMNamesResponse"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
