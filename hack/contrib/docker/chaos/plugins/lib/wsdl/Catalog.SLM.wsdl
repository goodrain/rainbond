         </documentation>
         
         <input message="zeusns:setResponseTimeByLocationRequest"/>
         <output message="zeusns:setResponseTimeByLocationResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
