         <fault name="DeploymentError" message="zeusns:DeploymentError"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
      <operation name="setSeriousThreshold">
         <documentation>
            Set the percentage of conforming responses below which a serious error will be emitted.
         </documentation>
         
         <input message="zeusns:setSeriousThresholdRequest"/>
         <output message="zeusns:setSeriousThresholdResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
