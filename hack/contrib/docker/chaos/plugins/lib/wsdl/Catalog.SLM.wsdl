      <operation name="getWarningThresholdByLocation">
         <documentation>
            Get the percentage of conforming responses below which a warning message will be triggered. This is a location specific function, any action will operate on the specified location.
         </documentation>
         
         <input message="zeusns:getWarningThresholdByLocationRequest"/>
         <output message="zeusns:getWarningThresholdByLocationResponse"/>
         <fault name="ObjectDoesNotExist" message="zeusns:ObjectDoesNotExist"/>
         <fault name="LicenseError" message="zeusns:LicenseError"/>
      </operation>
      
